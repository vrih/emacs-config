(defun my-buffer-face-mode-variable ()
   "Set font to a variable width (proportional) fonts in current buffer"
   (interactive)
   (setq buffer-face-mode-face '(:family "Noto Sans" :height 120 :width semi-condensed))
   (buffer-face-mode))

